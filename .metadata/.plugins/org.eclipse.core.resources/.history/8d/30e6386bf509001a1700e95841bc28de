package exercicio2.entities;

public abstract class Pessoa {
	private String nome;
	private Double anualIncome;

	public Pessoa() {
	}

	public Pessoa(String nome, Double rendeAnual) {
		super();
		this.nome = nome;
		this.anualIncome = rendeAnual;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Double getAnualIncome() {
		return anualIncome;
	}

	public void setAnualIncome(Double rendeAnual) {
		this.anualIncome = rendeAnual;
	}

	public abstract Double impostoDeRenda();

}
